<!--[if (gte IE 9)|!(IE)]><!--><html lang="en"> <!--<![endif]-->
<head>
   <!-- 最基本的 -->
  <!-- ================================================== -->
	<meta charset="utf-8">
	<title>AndyCall's blog</title>
	<meta name="description" content="欢迎来到andycall的小站">
	<meta name="author" content="AndyCall">
 
  <!-- 手机要用滴！ -->
  <!-- ================================================== -->
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  
  <!-- CSS -->
  <!-- ================================================== -->
  	<link rel="stylesheet" href="http://localhost:1500/stylesheets/base.css">
	<link rel="stylesheet" href="http://localhost:1500/stylesheets/skeleton.css">
	<link rel="stylesheet" href="http://localhost:1500/stylesheets/layout.css">
	<link rel="stylesheet" href="http://localhost:1500/stylesheets/screen.css">
	<!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->

	<!-- Favicons
	================================================== -->
	<link rel="shortcut icon" href="images/favicon.ico">
	<link rel="apple-touch-icon" href="images/apple-touch-icon.png">
	<link rel="apple-touch-icon" sizes="72x72" href="images/apple-touch-icon-72x72.png">
	<link rel="apple-touch-icon" sizes="114x114" href="images/apple-touch-icon-114x114.png">

</head>
<body>
	<header class="blog-head">
		<!-- <div class="vertical">
			<div class="blog-head-content inner">
				<h2>This is a Test</h2>
			</div>
		</div> -->

	</header>
	<main class="blog-main">
		<% docs.forEach(function(doc,index){ %>
		<article class="post tag-getting-started">
			<header class="post-header">
				<span class="post-meta"><%= doc.time.day %></span>
				<h2 class="post-title">
					<a href="/<%= doc.name %>/<%= doc.time.day %>/<%= doc.title %>"><%= doc.title %></a>
				</h2>
			</header>
			<section class='post-excerpt'>
				<%- doc.publish %>
			</section>
		</article>
		<% }); %>
	</main>
	<footer class="blog-footer">
		<div class="inner">
			<p>Powerd and Designed by andycall</p>
		</div>
	</footer>
</body>
<script type="text/javascript" src="javascripts/Observer.js"></script>
<script type="text/javascript" src="javascripts/Factory.js"></script>
<script type="text/javascript">

</script>
</html>